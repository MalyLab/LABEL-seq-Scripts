   "source": [
    "import math\n",
    "\n",
    "dataset = 'dmso1, WT normalized, BC level'\n",
    "variant = 'Gly466Glu'\n",
    "\n",
    "select = data[data['Label'] == variant]\n",
    "std = select[dataset].std()\n",
    "n = len(select[dataset])\n",
    "sqrt_n = math.sqrt(n)\n",
    "SEM = std/sqrt_n\n",
    "print('Mean +/- SEM = ' + \n",
    "      str(select[dataset].mean()) + \n",
    "      '+/-' + \n",
    "      str(SEM)\n",
    "     )"
   ]
